{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from \"vue\";\nimport jwt_decode from 'jwt-decode';\nimport MoreOptionsDialog from '@/components/Menu/MoreOptionsDialog.vue';\nexport default {\n  setup() {\n    const containerHeight = ref(1500); // 初始化容器高度\n    const minHeight = ref(1500); // 最小高度\n    const maxScrollHeight = ref(3000); // 最大滚动高度，控制界面的延伸\n\n    const handleScroll = event => {\n      if (event.deltaY > 0) {\n        // 向下滚动\n        if (containerHeight.value < maxScrollHeight.value) {\n          containerHeight.value += 50;\n        }\n      } else {\n        // 向上滚动\n        if (containerHeight.value > minHeight.value) {\n          containerHeight.value -= 50;\n        }\n      }\n    };\n    return {\n      handleScroll,\n      containerHeight,\n      minHeight,\n      maxScrollHeight\n    };\n  },\n  data() {\n    return {\n      selectedOptions: ['警员信息管理'],\n      myAuthority: jwt_decode(this.$route.query.token)['http://schemas.microsoft.com/ws/2008/06/identity/claims/role'],\n      myPoliceNumber: jwt_decode(this.$route.query.token)['http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name'],\n      dialogVisible: false\n    };\n  },\n  components: {\n    MoreOptionsDialog\n  },\n  methods: {\n    goTo(funcName) {\n      if (funcName == '注册') {\n        this.$router.push('/mainMenu/Register');\n      } else if (funcName == '警员权限修改') {\n        this.$router.push('/mainMenu/ChangePermission');\n      } else if (funcName == '警员考核管理') {\n        this.$router.push('/mainMenu/PoliceAssessment');\n      } else if (funcName == '警局信息管理') {\n        this.$router.push('/mainMenu/PoliceStationInfoManagement');\n      } else if (funcName == '警员信息管理') {\n        this.$router.push('/mainMenu/PoliceOfficerInfoManagement');\n      } else if (funcName == '警员出勤管理') {\n        this.$router.push('/mainMenu/AttendanceManagement');\n      } else if (funcName == '执法录像管理') {\n        this.$router.push('/mainMenu/VideoManagement');\n      } else if (funcName == '接警记录管理') {\n        this.$router.push('/mainMenu/AlarmResponseRecordManagement');\n      } else if (funcName == '薪水管理') {\n        this.$router.push('/mainMenu/PayrollManagement');\n      } else if (funcName == '证据收纳') {\n        this.$router.push('/mainMenu/EvidenceManagement');\n      } else if (funcName == '嫌疑人信息管理') {\n        this.$router.push('/mainMenu/SuspectInfoManagement');\n      } else if (funcName == '犯罪记录管理') {\n        this.$router.push('/mainMenu/CriminalRecordManagement');\n      } else if (funcName == '受害人信息管理') {\n        this.$router.push('/mainMenu/VictimInfoManagement');\n      } else if (funcName == '案件分类与管理') {\n        this.$router.push('/mainMenu/CaseClassificationManagement');\n      } else if (funcName == '犯罪数据统计') {\n        this.$router.push('/mainMenu/CrimeDataStatistics');\n      } else if (funcName == '重点人员统计') {\n        this.$router.push('/mainMenu/KeyIndividualsManagement');\n      } else if (funcName == '警车管理') {\n        this.$router.push('/mainMenu/PoliceVehicleManagement');\n      } else if (funcName == '警械管理') {\n        this.$router.push('/mainMenu/PoliceEquipmentManagement');\n      } else if (funcName == '公民户籍管理') {\n        this.$router.push('/mainMenu/CitizenInfoManagement');\n      } else if (funcName == '城市地区管理') {\n        this.$router.push('/mainMenu/UrbanZoningManagement');\n      } else if (funcName == '城市调度管理') {\n        this.$router.push('/mainMenu/RegionalDispatch');\n      } else if (funcName == '家族背景调查') {\n        this.$router.push('/mainMenu/FamilybgCheck');\n      } else if (funcName == '数据质量管理') {\n        this.$router.push('/mainMenu/DataQualityManagement');\n      } else if (funcName == '案件办理') {\n        this.$router.push('/mainMenu/CaseInvestigation');\n      } else if (funcName == '系统日志管理') {\n        this.$router.push('/mainMenu/SystemLogManagement');\n      }\n    },\n    showMoreOptions() {\n      this.$refs.moreOptionsDialog.showMoreOptions(this.selectedOptions, this.myAuthority);\n    },\n    handleOptionsSelected(selectedOptions) {\n      // 处理选项确认后的逻辑\n      // 更新按钮的显示状态等\n      this.selectedOptions = selectedOptions;\n      localStorage.setItem('selectedOptions_' + this.myPoliceNumber, JSON.stringify(selectedOptions));\n    },\n    handleMouseMove(event) {\n      const x = event.pageX - event.target.offsetLeft;\n      const y = event.pageY - event.target.offsetTop;\n      event.target.style.setProperty('--x', `${x}px`);\n      event.target.style.setProperty('--y', `${y}px`);\n    },\n    seetoken() {\n      console.log(this.myAuthority);\n      console.log(jwt_decode(this.$route.query.token));\n    }\n  },\n  created() {\n    const savedOptions = localStorage.getItem('selectedOptions_' + jwt_decode(this.$route.query.token)['http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name']);\n    if (savedOptions) {\n      console.log(savedOptions);\n      this.selectedOptions = JSON.parse(savedOptions);\n    }\n  }\n};","map":{"version":3,"names":["ref","jwt_decode","MoreOptionsDialog","setup","containerHeight","minHeight","maxScrollHeight","handleScroll","event","deltaY","value","data","selectedOptions","myAuthority","$route","query","token","myPoliceNumber","dialogVisible","components","methods","goTo","funcName","$router","push","showMoreOptions","$refs","moreOptionsDialog","handleOptionsSelected","localStorage","setItem","JSON","stringify","handleMouseMove","x","pageX","target","offsetLeft","y","pageY","offsetTop","style","setProperty","seetoken","console","log","created","savedOptions","getItem","parse"],"sources":["C:\\GitHubWarehouse\\2023-TJU-DBD-Police-Affair-System\\My-front\\dbproject\\src\\components\\Menu\\MainMenu.vue"],"sourcesContent":["<template>\r\n  <el-container\r\n    class=\"home-container\"\r\n    :style=\"{\r\n      height: containerHeight + 'px',\r\n      backgroundSize: '100% ' + containerHeight + 'px',\r\n    }\"\r\n    @wheel=\"handleScroll\"\r\n  >\r\n    <!-- 头部 -->\r\n    <el-header\r\n      style=\"height: 70px; padding: 0; text-align: justify; hyphens: auto\"\r\n    >\r\n      <div class=\"headpic\">\r\n        <div class=\"header1\">\r\n          <img class=\"logo\" src=\"../../assets/image-2.png\" />\r\n          <div class=\"title\">&nbsp;&nbsp;警务处理系统</div>\r\n          <div class=\"button-container\">\r\n            <el-button\r\n              type=\"primary\"\r\n              @click=\"\r\n                {\r\n                  drawer2 = true;\r\n                  console.log(containerHeight);\r\n                }\r\n              \"\r\n            >\r\n              个人信息\r\n            </el-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </el-header>\r\n\r\n    <!-- 页面主体 -->\r\n    <el-container>\r\n      <!-- 导航栏 -->\r\n      <el-aside width=\"200px\">\r\n        <el-menu\r\n          router\r\n          class=\"el-menu-vertical-demo\"\r\n          default-active=\"2\"\r\n          background-color=\"transparent\"\r\n          text-color=\"#ffffff\"\r\n          active-text-color=\"#ffd04b\"\r\n        >\r\n          <el-sub-menu index=\"1-1\">\r\n            <!-- 警员管理 -->\r\n            <template #title>\r\n              <span>\r\n                <img\r\n                  class=\"nav-icon-for-sub-menu\"\r\n                  src=\"../../assets/logos/PoliceManagement.png\"\r\n                  alt=\"Icon\"\r\n                />\r\n                警员管理</span\r\n              >\r\n            </template>\r\n            <el-menu-item v-if=\"myAuthority >= 1\"  index=\"/mainMenu/Register\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/Register.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              注册\r\n            </el-menu-item>\r\n            <el-menu-item v-if=\"myAuthority >= 2\"  index=\"/mainMenu/ChangePermission\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/ChangePermission.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              警员权限修改\r\n            </el-menu-item>\r\n            <el-menu-item v-if=\"myAuthority >= 1\"  index=\"/mainMenu/PoliceAssessment\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/PoliceAssessment.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              警员考核管理\r\n            </el-menu-item>\r\n            <el-menu-item v-if=\"myAuthority >= 4\"  index=\"/mainMenu/PoliceStationInfoManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/PoliceStationInfoManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              警局信息管理\r\n            </el-menu-item>\r\n            <el-menu-item v-if=\"myAuthority >= 0\"  index=\"/mainMenu/PoliceOfficerInfoManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/PoliceOfficerInfoManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              警员信息管理\r\n            </el-menu-item>\r\n            <el-menu-item  v-if=\"myAuthority >= 1\" index=\"/mainMenu/AttendanceManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/AttendanceManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              警员出勤管理\r\n            </el-menu-item>\r\n            <el-menu-item v-if=\"myAuthority >= 1\"  index=\"/mainMenu/VideoManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/VideoManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              执法录像管理\r\n            </el-menu-item>\r\n            <el-menu-item v-if=\"myAuthority >= 1\"  index=\"/mainMenu/AlarmResponseRecordManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/AlarmResponseRecordManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              接警记录管理\r\n            </el-menu-item>\r\n            <el-menu-item v-if=\"myAuthority >= 4\"  index=\"/mainMenu/PayrollManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/PayrollManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              薪水管理\r\n            </el-menu-item>\r\n          </el-sub-menu>\r\n\r\n          <!-- 案件管理 -->\r\n          <el-sub-menu v-if=\"myAuthority >= 1\"  index=\"1-2\">\r\n            <template #title>\r\n              <span>\r\n                <img\r\n                  class=\"nav-icon-for-sub-menu\"\r\n                  src=\"../../assets/logos/CaseManagement.png\"\r\n                  alt=\"Icon\"\r\n                />\r\n                案件管理</span\r\n              >\r\n            </template>\r\n            <el-menu-item index=\"/mainMenu/EvidenceManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/EvidenceManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              证据收纳\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/mainMenu/SuspectInfoManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/SuspectInfoManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              嫌疑人信息管理\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/mainMenu/CriminalRecordManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/CriminalRecordManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              犯罪记录管理\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/mainMenu/VictimInfoManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/VictimInfoManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              受害人信息管理\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/mainMenu/CaseClassificationManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/CaseClassificationManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              案件分类与管理\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/mainMenu/CrimeDataStatistics\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/CrimeDataStatistics.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              犯罪数据统计\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/mainMenu/KeyIndividualsManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/KeyIndividualsManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              重点人员统计\r\n            </el-menu-item>\r\n          </el-sub-menu>\r\n\r\n          <!-- 装备管理 -->\r\n          <el-sub-menu v-if=\"myAuthority >= 1\" index=\"1-3\">\r\n            <template #title>\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/EquipmentManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              <span>装备管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/mainMenu/PoliceVehicleManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/PoliceVehicleManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              警车管理\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/mainMenu/PoliceEquipmentManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/PoliceEquipmentManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              警械管理\r\n            </el-menu-item>\r\n          </el-sub-menu>\r\n\r\n          <!-- 城区和居民管理 -->\r\n\r\n          <el-sub-menu v-if=\"myAuthority >= 1\" index=\"1-4\">\r\n            <template #title>\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/UrbanResidentmanagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              <span>城区和居民管理</span>\r\n            </template>\r\n\r\n            <el-menu-item index=\"/mainMenu/CitizenInfoManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/CitizenInfoManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              公民户籍管理\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/mainMenu/UrbanZoningManagement\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/UrbanZoningManagement.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              城市地区管理\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/mainMenu/RegionalDispatch\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/RegionalDispatch.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              城市调度管理\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/mainMenu/FamilybgCheck\">\r\n              <img\r\n                class=\"nav-icon-for-menu-item\"\r\n                src=\"../../assets/logos/FamilybgCheck.png\"\r\n                alt=\"Icon\"\r\n              />\r\n              家族背景调查\r\n            </el-menu-item>\r\n          </el-sub-menu>\r\n\r\n          <el-menu-item v-if=\"myAuthority >= 1\" index=\"/mainMenu/DataQualityManagement\">\r\n            <img\r\n              class=\"nav-icon-for-menu-item\"\r\n              src=\"../../assets/logos/DataQualityManagement.png\"\r\n              alt=\"Icon\"\r\n            />\r\n            数据质量管理\r\n          </el-menu-item>\r\n          <el-menu-item v-if=\"myAuthority >= 1\" index=\"/mainMenu/CaseInvestigation\">\r\n            <img\r\n              class=\"nav-icon-for-menu-item\"\r\n              src=\"../../assets/logos/CaseInvestigation.png\"\r\n              alt=\"Icon\"\r\n            />\r\n            案件办理\r\n          </el-menu-item>\r\n\r\n          <!-- 系统日志 -->\r\n\r\n          <el-menu-item v-if=\"myAuthority >= 1\" index=\"/mainMenu/SystemLogManagement\">\r\n            <img\r\n              class=\"nav-icon-for-menu-item\"\r\n              src=\"../../assets/logos/SystemLogManagement.png\"\r\n              alt=\"Icon\"\r\n            />\r\n            系统日志管理\r\n          </el-menu-item>\r\n        </el-menu>\r\n      </el-aside>\r\n\r\n      <!-- 页面主体 -->\r\n      <el-main class=\"background\">\r\n        <div style=\"width:60%;display:inline-block;margin-left:20px\">\r\n          <div style=\"height:200px;font: 400 25px Inter, sans-serif;vertical-align: top;\">\r\n            <div><br>欢迎回来，{{ myPoliceNumber }}<br></div>\r\n            <div style=\"font: 400 20px Inter, sans-serif;\"><br>开始愉快的一天</div>\r\n          </div>\r\n          <div class=\"syslogtitle\">\r\n            <div class=\"syslog\">系统日志</div>\r\n            <button style=\"height:80%;position: absolute;width:50px;right:20px;background-color:#c1b598;color:white;cursor: pointer;border: none;\">更多</button>\r\n          </div>\r\n        </div>\r\n        <div style=\"width:31%;min-height: 100px;margin-left:5%;display:inline-b