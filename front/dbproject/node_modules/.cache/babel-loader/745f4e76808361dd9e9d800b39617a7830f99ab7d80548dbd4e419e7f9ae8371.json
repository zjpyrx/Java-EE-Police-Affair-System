{"ast":null,"code":"import axios from 'axios';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      anv: \"1\",\n      showCase: false,\n      suspectID: '',\n      suspectName: '',\n      suspectSex: '',\n      suspectAddress: '',\n      suspectInfo: [],\n      chanSuspectInfo: [],\n      caseInfo: [],\n      addSuspectID: \"\",\n      addCaseID: \"\",\n      addCaseType: \"\",\n      type1: \"\",\n      delSuspectID: \"\",\n      delCaseID: \"\",\n      delCaseType: \"\",\n      type2: \"\"\n    };\n  },\n  methods: {\n    handleMouseMove(event) {\n      const x = event.pageX - event.target.offsetLeft;\n      const y = event.pageY - event.target.offsetTop;\n      event.target.style.setProperty(\"--x\", `${x}px`);\n      event.target.style.setProperty(\"--y\", `${y}px`);\n    },\n    async fetchSuspectInfo() {\n      try {\n        const response = await axios.get(`http://localhost:7078/api/suspectInfo?suspectID=${encodeURIComponent(this.suspectID)}&suspectName=${encodeURIComponent(this.suspectName)}&suspectSex=${encodeURIComponent(this.suspectSex)}&suspectAddress=${encodeURIComponent(this.suspectAddress)}`);\n        this.suspectInfo = response.data;\n        this.chanSuspectInfo = this.suspectInfo.map(item => {\n          if (item.sex === 'M') {\n            return {\n              ...item,\n              sex: '男'\n            };\n          } else if (item.sex === 'F') {\n            return {\n              ...item,\n              sex: '女'\n            };\n          } else {\n            return item;\n          }\n        });\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"获取嫌疑人列表失败！\",\n          type: 'error',\n          duration: 5000\n        });\n      }\n    },\n    async getCase(a, suspect) {\n      try {\n        const response = await axios.get(`http://localhost:7078/api/getCaseS?suspectID=${encodeURIComponent(suspect.id)}`);\n        this.caseInfo = response.data;\n        this.showCase = true;\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"获取案件信息失败！\",\n          type: 'error',\n          duration: 5000\n        });\n      }\n    },\n    async addSuspectInfo() {\n      try {\n        const response = await axios.post(`http://localhost:7078/api/addSuspect?suspectID=${encodeURIComponent(this.addSuspectID)}&caseID=${encodeURIComponent(this.addCaseID)}&caseType=${encodeURIComponent(this.addCaseType)}`);\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: 'warning',\n            duration: 5000\n          });\n        } else {\n          await this.fetchSuspectInfo();\n          ElMessage({\n            showClose: true,\n            message: '新增嫌疑人成功! ',\n            type: 'success',\n            duration: 5000\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"新增嫌疑人失败！\",\n          type: 'error',\n          duration: 5000\n        });\n      }\n    },\n    async delSuspectInfo() {\n      try {\n        const response = await axios.delete(`http://localhost:7078/api/delSuspect?suspectID=${encodeURIComponent(this.delSuspectID)}&caseID=${encodeURIComponent(this.delCaseID)}&caseType=${encodeURIComponent(this.delCaseType)}`);\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: 'warning',\n            duration: 5000\n          });\n        } else {\n          await this.fetchSuspectInfo();\n          ElMessage({\n            showClose: true,\n            message: '删除嫌疑人成功! ',\n            type: 'success',\n            duration: 5000\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"删除嫌疑人失败！\",\n          type: 'error',\n          duration: 5000\n        });\n      }\n    },\n    async back() {\n      this.caseInfo = \"\";\n      this.showCase = false;\n    },\n    typeToID(type) {\n      if (type === \"\") {\n        return \"\";\n      } else if (type === \"强奸\") {\n        return \"A\";\n      } else if (type === \"抢劫\") {\n        return \"R\";\n      } else if (type === \"故意伤害\") {\n        return \"H\";\n      } else if (type === \"盗窃\") {\n        return \"T\";\n      } else if (type === \"诈骗\") {\n        return \"C\";\n      } else if (type === \"谋杀\") {\n        return \"M\";\n      } else {\n        ElMessage({\n          showClose: true,\n          message: \"案件类型转换错误！\",\n          type: 'error',\n          duration: 5000\n        });\n        return \"\";\n      }\n    }\n  },\n  watch: {\n    addCaseType() {\n      this.type1 = this.typeToID(this.addCaseType);\n    },\n    delCaseType() {\n      this.type2 = this.typeToID(this.delCaseType);\n    }\n  }\n};","map":{"version":3,"names":["axios","ElMessage","data","anv","showCase","suspectID","suspectName","suspectSex","suspectAddress","suspectInfo","chanSuspectInfo","caseInfo","addSuspectID","addCaseID","addCaseType","type1","delSuspectID","delCaseID","delCaseType","type2","methods","handleMouseMove","event","x","pageX","target","offsetLeft","y","pageY","offsetTop","style","setProperty","fetchSuspectInfo","response","get","encodeURIComponent","map","item","sex","error","showClose","message","type","duration","getCase","a","suspect","id","addSuspectInfo","post","delSuspectInfo","delete","back","typeToID","watch"],"sources":["D:\\Work\\大二下\\DB小学期\\gitspace11\\2023-TJU-DBD-Police-Affair-System\\My-front\\dbproject\\src\\components\\CaseManagement\\SuspectInfoManagement.vue"],"sourcesContent":["<template>\r\n  <el-header class=\"sub-header\" @mousemove=\"handleMouseMove\">\r\n    <div>&nbsp;&nbsp;案件管理&nbsp;>&nbsp;嫌疑人信息管理</div>\r\n  </el-header>\r\n\r\n  <el-tabs v-model=\"anv\" type=\"border-card\" style=\"margin-top: 10vh;\">\r\n    <!-- 查询嫌疑人栏 -->\r\n    <el-tab-pane label=\"查询嫌疑人\" name=\"1\">\r\n      <div style=\"text-align: center;\">\r\n        <table style=\"display: inline-block;text-align: left;\">\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">姓名：</el-text>\r\n              <el-input class=\"inputBox\" v-model=\"suspectName\" placeholder=\"请输入姓名\" />\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">性别：</el-text>\r\n              <el-select class=\"inputBox\" v-model=\"suspectSex\" placeholder=\"请选择\">\r\n                <el-option selected label=\"全部性别\" value=\"\"></el-option>\r\n                <el-option label=\"男\" value=\"M\"></el-option>\r\n                <el-option label=\"女\" value=\"F\"></el-option>\r\n              </el-select>\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">住址：</el-text>\r\n              <el-input class=\"inputBox\" v-model=\"suspectAddress\" placeholder=\"请输入住址\" />\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">身份证号：</el-text>\r\n              <el-input class=\"inputBox\" v-model=\"suspectID\" placeholder=\"请输入身份证号码\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\" clearable maxlength=\"18\" show-word-limit />\r\n            </td>\r\n          </tr>\r\n        </table>\r\n        <div class=\"seaButton\">\r\n          <el-button type=\"primary\" @click=\"fetchSuspectInfo\">查询</el-button>\r\n        </div>\r\n      </div>\r\n\r\n      <el-divider />\r\n      <div>\r\n        <!-- 表格显示获取的嫌疑人信息 -->\r\n        <el-table v-if=\"!showCase\" :data=\"chanSuspectInfo\" stripe height=\"450\" @wheel.passive.stop>\r\n          <el-table-column prop=\"id\" label=\"身份证号\" sortable width=\"250px\" />\r\n          <el-table-column prop=\"name\" label=\"姓名\" sortable width=\"150px\" />\r\n          <el-table-column prop=\"sex\" label=\"性别\" sortable width=\"150px\" />\r\n          <el-table-column prop=\"address\" label=\"住址\" sortable />\r\n          <el-table-column align=\"right\" width=\"100px\">\r\n            <template #header></template>\r\n            <template #default=\"scope\">\r\n              <el-button size=\"small\" type=\"primary\" @click=\"getCase(scope.$index, scope.row)\">涉及案件</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <!-- 案件列表 -->\r\n        <el-table v-if=\"showCase && caseInfo.length > 0\" :data=\"caseInfo\" stripe height=\"450\" @wheel.passive.stop>\r\n          <el-table-column prop=\"id\" label=\"案件编号\" sortable width=\"120px\" />\r\n          <el-table-column prop=\"type\" label=\"案件类型\" sortable width=\"120px\" />\r\n          <el-table-column prop=\"status\" label=\"案件状态\" sortable width=\"120px\" />\r\n          <el-table-column prop=\"registerTime\" label=\"登记时间\" sortable />\r\n          <el-table-column prop=\"address\" label=\"案发地点\" sortable />\r\n          <el-table-column prop=\"ranking\" label=\"案件等级\" sortable width=\"120px\" />\r\n          <el-table-column align=\"center\" width=\"80px\">\r\n            <template #header>\r\n              <el-button size=\"small\" type=\"primary\" @click=\"back\">返回</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n    </el-tab-pane>\r\n\r\n    <!-- 新增嫌疑人栏 -->\r\n    <el-tab-pane label=\"新增嫌疑人\" name=\"2\">\r\n      <div style=\"text-align: center;\">\r\n        <table style=\"display: inline-block;text-align: left;\">\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\" style=\"margin-right: 10px;\">案件编号:</el-text>\r\n              <el-input v-model=\"addCaseID\" placeholder=\"案件编号\" clearable maxlength=\"7\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\" show-word-limit style=\"width: 190px;\">\r\n                <template #prepend>\r\n                  <div class=\"qianru\">{{ type1 }}</div>\r\n                </template>\r\n              </el-input>\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">案件类型:</el-text>\r\n              <el-select class=\"inputBox\" v-model=\"addCaseType\" placeholder=\"请选择\">\r\n                <el-option label=\"强奸\" value=\"强奸\"></el-option>\r\n                <el-option label=\"抢劫\" value=\"抢劫\"></el-option>\r\n                <el-option label=\"故意伤害\" value=\"故意伤害\"></el-option>\r\n                <el-option label=\"盗窃\" value=\"盗窃\"></el-option>\r\n                <el-option label=\"诈骗\" value=\"诈骗\"></el-option>\r\n                <el-option label=\"谋杀\" value=\"谋杀\"></el-option>\r\n              </el-select>\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\" style=\"margin-left: 7px;\">身份证号：</el-text>\r\n              <el-input class=\"inputBox\" v-model=\"addSuspectID\" placeholder=\"请输入身份证号\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\" style=\"margin-left: 3px;\" clearable maxlength=\"18\"\r\n                show-word-limit />\r\n            </td>\r\n          </tr>\r\n        </table>\r\n\r\n        <div class=\"seaButton\">\r\n          <el-button type=\"primary\" @click=\"addSuspectInfo\">新增嫌疑人</el-button>\r\n        </div>\r\n      </div>\r\n    </el-tab-pane>\r\n\r\n    <!-- 删除嫌疑人栏 -->\r\n    <el-tab-pane label=\"删除嫌疑人\" name=\"3\">\r\n      <div style=\"text-align: center;\">\r\n        <table style=\"display: inline-block;text-align: left;\">\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\" style=\"margin-right: 10px;\">案件编号:</el-text>\r\n              <el-input v-model=\"delCaseID\" placeholder=\"案件编号\" clearable maxlength=\"7\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\" show-word-limit style=\"width: 190px;\">\r\n                <template #prepend>\r\n                  <div class=\"qianru\">{{ type2 }}</div>\r\n                </template>\r\n              </el-input>\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">案件类型:</el-text>\r\n              <el-select class=\"inputBox\" v-model=\"delCaseType\" placeholder=\"请选择\">\r\n                <el-option label=\"强奸\" value=\"强奸\"></el-option>\r\n                <el-option label=\"抢劫\" value=\"抢劫\"></el-option>\r\n                <el-option label=\"故意伤害\" value=\"故意伤害\"></el-option>\r\n                <el-option label=\"盗窃\" value=\"盗窃\"></el-option>\r\n                <el-option label=\"诈骗\" value=\"诈骗\"></el-option>\r\n                <el-option label=\"谋杀\" value=\"谋杀\"></el-option>\r\n              </el-select>\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\" style=\"margin-left: 7px;\">身份证号：</el-text>\r\n              <el-input class=\"inputBox\" v-model=\"delSuspectID\" placeholder=\"请输入身份证号\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\" style=\"margin-left: 3px;\" clearable maxlength=\"18\"\r\n                show-word-limit />\r\n            </td>\r\n          </tr>\r\n        </table>\r\n\r\n        <div class=\"seaButton\">\r\n          <el-button type=\"primary\" @click=\"delSuspectInfo\">删除嫌疑人</el-button>\r\n        </div>\r\n      </div>\r\n    </el-tab-pane>\r\n  </el-tabs>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { ElMessage } from 'element-plus';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      anv: \"1\",\r\n      showCase: false,\r\n      suspectID: '',\r\n      suspectName: '',\r\n      suspectSex: '',\r\n      suspectAddress: '',\r\n      suspectInfo: [],\r\n      chanSuspectInfo: [],\r\n      caseInfo: [],\r\n      addSuspectID: \"\",\r\n      addCaseID: \"\",\r\n      addCaseType: \"\",\r\n      type1: \"\",\r\n      delSuspectID: \"\",\r\n      delCaseID: \"\",\r\n      delCaseType: \"\",\r\n      type2: \"\",\r\n    }\r\n  },\r\n  methods: {\r\n    handleMouseMove(event) {\r\n      const x = event.pageX - event.target.offsetLeft;\r\n      const y = event.pageY - event.target.offsetTop;\r\n\r\n      event.target.style.setProperty(\"--x\", `${x}px`);\r\n      event.target.style.setProperty(\"--y\", `${y}px`);\r\n    },\r\n    async fetchSuspectInfo() {\r\n      try {\r\n        const response = await axios.get(`http://localhost:7078/api/suspectInfo?suspectID=${encodeURIComponent(this.suspectID)}&suspectName=${encodeURIComponent(this.suspectName)}&suspectSex=${encodeURIComponent(this.suspectSex)}&suspectAddress=${encodeURIComponent(this.suspectAddress)}`);\r\n        this.suspectInfo = response.data;\r\n        this.chanSuspectInfo = this.suspectInfo.map(item => {\r\n          if (item.sex === 'M') {\r\n            return { ...item, sex: '男' };\r\n          } else if (item.sex === 'F') {\r\n            return { ...item, sex: '女' };\r\n          } else {\r\n            return item;\r\n          }\r\n        });\r\n      } catch (error) {\r\n        // 请求失败时的处理逻辑\r\n        ElMessage({\r\n          showClose: true,\r\n          message: \"获取嫌疑人列表失败！\",\r\n          type: 'error',\r\n          duration: 5000,\r\n        });\r\n      }\r\n    },\r\n    async getCase(a, suspect) {\r\n      try {\r\n        const response = await axios.get(`http://localhost:7078/api/getCaseS?suspectID=${encodeURIComponent(suspect.id)}`);\r\n        this.caseInfo = response.data;\r\n        this.showCase = true;\r\n      } catch (error) {\r\n        // 请求失败时的处理逻辑\r\n        ElMessage({\r\n          showClose: true,\r\n          message: \"获取案件信息失败！\",\r\n          type: 'error',\r\n          duration: 5000,\r\n        });\r\n      }\r\n    },\r\n    async addSuspectInfo() {\r\n      try {\r\n        const response = await axios.post(`http://localhost:7078/api/addSuspect?suspectID=${encodeURIComponent(this.addSuspectID)}&caseID=${encodeURIComponent(this.addCaseID)}&caseType=${encodeURIComponent(this.addCaseType)}`);\r\n        if (typeof response.data == \"string\") {\r\n          ElMessage({\r\n            showClose: true,\r\n            message: response.data,\r\n            type: 'warning',\r\n            duration: 5000,\r\n          });\r\n        }\r\n        else {\r\n          await this.fetchSuspectInfo();\r\n          ElMessage({\r\n            showClose: true,\r\n            message: '新增嫌疑人成功! ',\r\n            type: 'success',\r\n            duration: 5000,\r\n          });\r\n        }\r\n      } catch (error) {\r\n        // 请求失败时的处理逻辑\r\n        ElMessage({\r\n          showClose: true,\r\n          message: \"新增嫌疑人失败！\",\r\n          type: 'error',\r\n          duration: 5000,\r\n        });\r\n      }\r\n    },\r\n    async delSuspectInfo() {\r\n      try {\r\n        const response = await axios.delete(`http://localhost:7078/api/delSuspect?suspectID=${encodeURIComponent(this.delSuspectID)}&caseID=${encodeURIComponent(this.delCaseID)}&caseType=${encodeURIComponent(this.delCaseType)}`);\r\n        if (typeof response.data == \"string\") {\r\n          ElMessage({\r\n            showClose: true,\r\n            message: response.data,\r\n            type: 'warning',\r\n            duration: 5000,\r\n          });\r\n        }\r\n        else {\r\n          await this.fetchSuspectInfo();\r\n          ElMessage({\r\n            showClose: true,\r\n            message: '删除嫌疑人成功! ',\r\n            type: 'success',\r\n            duration: 5000,\r\n          });\r\n        }\r\n      } catch (error) {\r\n        // 请求失败时的处理逻辑\r\n        ElMessage({\r\n          showClose: true,\r\n          message: \"删除嫌疑人失败！\",\r\n          type: 'error',\r\n          duration: 5000,\r\n        });\r\n      }\r\n    },\r\n    async back() {\r\n      this.caseInfo = \"\";\r\n      this.showCase = false;\r\n    },\r\n    typeToID(type) {\r\n      if (type === \"\") {\r\n        return (\"\");\r\n      }\r\n      else if (type === \"强奸\") {\r\n        return (\"A\");\r\n      }\r\n      else if (type === \"抢劫\") {\r\n        return (\"R\");\r\n      }\r\n      else if (type === \"故意伤害\") {\r\n        return (\"H\");\r\n      }\r\n      else if (type === \"盗窃\") {\r\n        return (\"T\");\r\n      }\r\n      else if (type === \"诈骗\") {\r\n        return (\"C\");\r\n      }\r\n      else if (type === \"谋杀\") {\r\n        return (\"M\");\r\n      }\r\n      else {\r\n        ElMessage({\r\n          showClose: true,\r\n          message: \"案件类型转换错误！\",\r\n          type: 'error',\r\n          duration: 5000,\r\n        });\r\n        return (\"\");\r\n      }\r\n    },\r\n  },\r\n  watch: {\r\n    addCaseType() {\r\n      this.type1 = this.typeToID(this.addCaseType);\r\n    },\r\n    delCaseType() {\r\n      this.type2 = this.typeToID(this.delCaseType);\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"postcss\" scoped>\r\n.sub-header {\r\n  overflow: hidden;\r\n  display: flex;\r\n  position: absolute;\r\n  top: 70px;\r\n  left: 199px;\r\n  width: calc(100% - 199px);\r\n  height: 7vh;\r\n  min-height: 40px;\r\n  align-items: center;\r\n  /* 文字竖直方向居中对齐 */\r\n  background-color: #1f2cdf;\r\n  box-shadow: inset -500px 0px 200px 0px rgba(4, 0, 113, 0.856);\r\n  color: #ffffff;\r\n  font-size: 28px;\r\n}\r\n\r\n.sub-header::before {\r\n  --size: 0;\r\n  content: '';\r\n  position: absolute;\r\n  left: var(--x);\r\n  top: var(--y);\r\n  width: var(--size);\r\n  height: var(--size);\r\n  background: radial-gradient(circle closest-side, #5a65ff, transparent);\r\n  transform: translate(-50%, -50%);\r\n  transition: width .2s ease, height .2s ease;\r\n}\r\n\r\n.sub-header:hover::before {\r\n  --size: 400px;\r\n}\r\n\r\n.sea